
public class SampleObject {
	private int fileNo;
	private int word;
	public int getFileNo() {
		return fileNo;
	}
	public void setFileNo(int fileNo) {
		this.fileNo = fileNo;
	}
	public SampleObject(int fileNo, int word) {
		super();
		this.fileNo = fileNo;
		this.word = word;
	}
	public int getWord() {
		return word;
	}
	public void setWord(int word) {
		this.word = word;
	}
	
}
